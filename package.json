  "license": "ISC",
  "dependencies": {
    "express": "^4.18.1",
    "joi": "^17.6.1",
    "sequelize": "^6.23.0",
    "sqlite3": "^5.1.1"
  },
  "devDependencies": {
    "dotenv": "^16.0.2",
    "nodemon": "^2.0.20",
    "sequelize-cli": "^6.4.1"
  }
}
