{
  "name": "inventory_managament",
  "version": "1.0.0",
  "description": "Nodejs Fundamentals",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "nodemon src/index.js",
    "generate": "npx sequelize-cli migration:generate --name ims",
    "migrations": "npx sequelize db:migrate",
    "migrations:undo": "npx sequelize db:migrate:undo",
    "seed": "npx sequelize seed:generate --name ims",
    "seed:all": "npx sequelize-cli db:seed:all",
    "seed:undo": "npx sequelize-cli db:seed:undo:all"
  },
